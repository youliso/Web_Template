{
  "port": 3000,
  "socketPath": "/io",
  "domainWhiteList": [
    "http://localhost:3000"
  ],
  "cors": {
    "allowMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS"
    ],
    "allowHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "exposeHeaders": [
      "Content-Type",
      "Authorization"
    ]
  },
  "noToken": [
    "/index/get",
    "/user/login",
    "/user/register"
  ]
}
